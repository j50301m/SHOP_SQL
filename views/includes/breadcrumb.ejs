<% var breadcrumb; %> 
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
    <% if(breadcrumb){ %> 
      <% breadcrumb.forEach(function(item){ %> 
      <li class="breadcrumb-item active" aria-current="page">
          <% if(item.hasBreadcrumbUrl){%>
                <a href=<%= item.url %> ><%= item.name  %> </a>
          <% }else{ %> 
                <%= item.name  %>
          <% } %> 
      </li>
      <% }) %> 
    <% } %> 
    </ol>
  </nav>